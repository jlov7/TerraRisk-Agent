# TerraRisk Agent backend configuration
# Copy to .env and adjust as needed.

# Application
APP_NAME="TerraRisk Agent (Personal R&D)"
ENVIRONMENT=local

# Feature flags
EARTH_AI_ENABLED=0

# Google Cloud / BigQuery (set when running BYO BigQuery or cloud modes)
GCP_PROJECT=
BQ_DATASET=
EARTHENGINE_PROJECT=

# Artifact output directory (relative to backend package by default)
ARTIFACT_DIR=examples/artifacts

# Observability
OTEL_EXPORTER_OTLP_ENDPOINT=http://otel-collector:4317
